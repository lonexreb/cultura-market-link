{
  "workflow": {
    "id": "test-api-search-graphrag",
    "name": "API, Search, and GraphRAG Workflow",
    "description": "Tests the newly implemented API, Search, and GraphRAG node types",
    "nodes": [
      {
        "id": "doc-source",
        "type": "document",
        "position": {"x": 100, "y": 100},
        "data": {
          "label": "Document Source",
          "description": "Source document for processing"
        },
        "config": {
          "text": "OpenAI released GPT-4 which revolutionized artificial intelligence. The company was founded by Sam Altman and others in Silicon Valley. Microsoft invested billions in OpenAI to advance AI research. Google also developed Gemini to compete with GPT models.",
          "chunk_size": 200,
          "extract_entities": true
        }
      },
      {
        "id": "graphrag-extract",
        "type": "graphrag",
        "position": {"x": 300, "y": 100},
        "data": {
          "label": "Extract Knowledge",
          "description": "Extract entities and relationships from document"
        },
        "config": {
          "operation": "extract",
          "auto_store": true
        }
      },
      {
        "id": "search-web",
        "type": "search",
        "position": {"x": 500, "y": 50},
        "data": {
          "label": "Web Search",
          "description": "Search for additional information"
        },
        "config": {
          "search_type": "web",
          "query": "OpenAI GPT-4 latest news",
          "max_results": 3
        }
      },
      {
        "id": "search-doc",
        "type": "search",
        "position": {"x": 500, "y": 150},
        "data": {
          "label": "Document Search",
          "description": "Search within processed documents"
        },
        "config": {
          "search_type": "document",
          "query": "OpenAI",
          "max_results": 5
        }
      },
      {
        "id": "api-call",
        "type": "api",
        "position": {"x": 700, "y": 100},
        "data": {
          "label": "API Call",
          "description": "Make external API call"
        },
        "config": {
          "url": "https://jsonplaceholder.typicode.com/posts/1",
          "method": "GET",
          "headers": {
            "Content-Type": "application/json",
            "User-Agent": "AgentOps-Workflow/1.0"
          },
          "timeout": 10
        }
      },
      {
        "id": "graphrag-query",
        "type": "graphrag",
        "position": {"x": 900, "y": 100},
        "data": {
          "label": "Query Knowledge Graph",
          "description": "Query the built knowledge graph"
        },
        "config": {
          "operation": "query",
          "query": "OpenAI"
        }
      }
    ],
    "edges": [
      {
        "id": "e1",
        "source": "doc-source",
        "target": "graphrag-extract",
        "type": "default"
      },
      {
        "id": "e2",
        "source": "graphrag-extract",
        "target": "search-web",
        "type": "default"
      },
      {
        "id": "e3",
        "source": "doc-source",
        "target": "search-doc",
        "type": "default"
      },
      {
        "id": "e4",
        "source": "search-web",
        "target": "api-call",
        "type": "default"
      },
      {
        "id": "e5",
        "source": "graphrag-extract",
        "target": "graphrag-query",
        "type": "default"
      }
    ]
  },
  "debug": true,
  "minimize_logs": false
} 